ui_print("");
show_progress(1.000000, 0);

run_program("/sbin/mount", "/system");
set_progress(0.250000);

run_program("/sbin/mount", "/cache");
set_progress(0.500000);

package_extract_file("xzdrremove.sh", "/sbin/xzdrremove.sh");
set_perm(0, 0, 0755, "/sbin/xzdrremove.sh");
run_program("/sbin/xzdrremove.sh");
set_progress(0.750000);

ui_print("  Unmounting Partitions...");
unmount("/system");
set_progress(0.950000);

ui_print("FINISHED!");
set_progress(1.000000);

