ui_print("");ui_print("");ui_print("");
ui_print("===========================================");
ui_print("|                                         |");
ui_print("| *** XZDualRecovery Removal tool***      |");
ui_print("|                                         |");
ui_print("|              By Shoey63                 |");
ui_print("|       Maintained by [NUT] on XDA        |");
ui_print("|                                         |");
